{
    "version": 2,
    "builds": [
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/script.js",
        "use": "@vercel/static"
      },
      {
        "src": "images/**",
        "use": "@vercel/static"
      },
      {
        "src": "audio/**",
        "use": "@vercel/static"
      },
      {
        "src": "*.json",
        "use": "@vercel/static"
      },
      {
        "src": "*.py",
        "use": "@vercel/python"
      }
    ]
}

  